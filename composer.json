{
  "license": "proprietary",
  "name": "prinik/front-test",
  "type": "project",
  "autoload": {
    "psr-4": {
      "PriNikApp\\FrontTest\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PriNikTests\\FrontTest\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Somewho",
      "email": "somewho@gmail.com"
    },
    {
      "name": "PriNik",
      "email": "akchapocha@gmail.com"
    }
  ],
  "require": {
    "ext-pdo": "*",
    "ext-json": "*",
    "ext-zip": "*",
    "php": "^8.1",
    "php-di/php-di": "^6.4",
    "slim/psr7": "^1.6",
    "slim/slim": "^4.11",
    "twig/twig": "^3.5",
    "devcoder-xyz/php-dotenv": "^1.1",
    "doctrine/orm": "^2.14",
    "doctrine/migrations":  "^3.6",
    "symfony/cache": "^6.2"
  },
  "require-dev": {
    "phpstan/extension-installer": "^1.2",
    "phpstan/phpstan": "^1.10",
    "phpunit/phpunit": "^10.0",
    "squizlabs/php_codesniffer": "^3.7"
  },
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true
    },
    "process-timeout": 0,
    "sort-packages": true
  }
}
